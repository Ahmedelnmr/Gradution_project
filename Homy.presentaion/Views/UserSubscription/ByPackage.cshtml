@model IEnumerable<Homy.Application.Dtos.UserDtos.UserSubDTOs.UserSubReadDTO>

@{
    ViewData["Title"] = "اشتراكات الباقة";
}

<h2>اشتراكات الباقة</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>المستخدم</th>
            <th>تاريخ البدء</th>
            <th>تاريخ الانتهاء</th>
            <th>نشط</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserId</td>
                <td>@item.StartDate.ToShortDateString()</td>
                <td>@item.EndDate.ToShortDateString()</td>
                <td>@item.IsActive</td>
            </tr>
        }
    </tbody>
</table>
