@model IEnumerable<Homy.Application.Dtos.UserDtos.UserSubDTOs.UserSubReadDTO>

@{
    ViewData["Title"] = "الاشتراكات المنتهية";
}

<h2>الاشتراكات المنتهية</h2>

@if (!Model.Any())
{
    <p>لا توجد اشتراكات منتهية</p>
}
else
{
    <table class="table table-danger">
        <tr>
            <th>المستخدم</th>
            <th>تاريخ الانتهاء</th>
        </tr>
        @foreach (var sub in Model)
        {
            <tr>
                <td>@sub.UserName</td>
                <td>@sub.EndDate.ToShortDateString()</td>
            </tr>
        }
    </table>
}
